{
  "action": {
    "default_icon": {
      "19": "graphics/codeclimbers-19.png",
      "38": "graphics/codeclimbers-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Code Climbers"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["codeclimbersScript.js"],
      "run_at": "document_end"
    }
  ],
  "description": "Automatic time tracking for Edge.",
  "devtools_page": "devtools.html",
  "homepage_url": "http://localhost:14400",
  "host_permissions": ["http://localhost:14400/*"],
  "icons": {
    "16": "graphics/codeclimbers-16.png",
    "48": "graphics/codeclimbers-48.png",
    "128": "graphics/codeclimbers-128.png"
  },
  "manifest_version": 3,
  "name": "Code Climbers",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["alarms", "tabs", "storage", "idle"],
  "version": "3.0.22"
}
