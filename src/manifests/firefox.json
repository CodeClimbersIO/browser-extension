{
  "background": {
    "persistent": false,
    "scripts": ["public/js/browser-polyfill.min.js", "background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "graphics/codeclimbers-19.png",
      "38": "graphics/codeclimbers-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Code Climbers"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@codeclimbers.com",
      "strict_min_version": "48.0"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["codeclimbersScript.js"],
      "run_at": "document_end"
    }
  ],
  "description": "Automatic time tracking for Firefox.",
  "devtools_page": "devtools.html",
  "homepage_url": "http://localhost:14400",
  "icons": {
    "16": "graphics/codeclimbers-16.png",
    "48": "graphics/codeclimbers-48.png",
    "128": "graphics/codeclimbers-128.png"
  },
  "manifest_version": 2,
  "name": "Code Climbers",
  "options_ui": {
    "chrome_style": false,
    "page": "options.html"
  },
  "permissions": ["<all_urls>", "alarms", "tabs", "storage", "idle"],
  "version": "3.0.22"
}
